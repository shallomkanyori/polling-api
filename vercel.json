{
    "builds": [
        {
            "src": "polling_api/wsgi.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "buildCommand": "pip install -r requirements.txt && python manage.py migrate"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/polling_api/wsgi.py"
        }
    ]
}